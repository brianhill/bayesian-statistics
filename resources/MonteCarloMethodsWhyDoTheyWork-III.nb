   3.942482375346265*^9, {3.942569186070477*^9, 3.942569478246037*^9}, {
   3.942569508264987*^9, 3.942569522675412*^9}, {3.942569580985352*^9, 
   3.9425696027349253`*^9}, {3.942569641109827*^9, 3.942569675082542*^9}, {
   3.942569706617342*^9, 3.9425697808512783`*^9}, {3.942569816317915*^9, 
   3.942570245668092*^9}, {3.942570333273471*^9, 3.942570600283627*^9}, {
   3.94257069740199*^9, 3.942570925091745*^9}, {3.9425714748078547`*^9, 
   3.942571514733362*^9}, {3.9425723378445473`*^9, 3.942572346798588*^9}, {
   3.942572502675579*^9, 3.9425725044868393`*^9}, {3.942573257427242*^9, 
   3.942573282888261*^9}, {3.9425733344293203`*^9, 3.942573368818153*^9}, {
   3.9425762303238163`*^9, 
   3.94257623078305*^9}},ExpressionUUID->"28a71991-cb0d-40e9-a92f-\
