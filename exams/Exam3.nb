  3.9403793151643248`*^9, 3.940379331259646*^9}, {3.940379364052906*^9, 
  3.940379404566983*^9}, {3.940381431528981*^9, 3.94038143433399*^9}, {
  3.940402436343687*^9, 3.940402459637848*^9}, {3.943051433905607*^9, 
  3.943051466469449*^9}, {3.943051759803689*^9, 3.943051764331042*^9}, {
  3.943052370355011*^9, 3.943052370503951*^9}, {3.9430529759828377`*^9, 
  3.943052997355608*^9}, {3.943054523073599*^9, 3.943054540561843*^9}, {
  3.943055815607552*^9, 
