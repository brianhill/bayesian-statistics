   3.943051466469449*^9}, {3.943051759803689*^9, 3.943051764331042*^9}, {
   3.943052370355011*^9, 3.943052370503951*^9}, {3.9430529759828377`*^9, 
   3.943052997355608*^9}, {3.943054523073599*^9, 3.943054540561843*^9}, {
   3.943055815607552*^9, 3.9430558158189487`*^9}, {3.9430560663669653`*^9, 
   3.943056077717987*^9}, {3.94305738381636*^9, 3.943057389212532*^9}, {
   3.943057831927658*^9, 3.943057845376483*^9}, {3.943081328605784*^9, 
